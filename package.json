{
  "name": "nest-sequelize-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "make:seed": "node_modules/.bin/sequelize seed:create --name $NAME",
    "db:seed": "node_modules/.bin/sequelize db:seed --seed $NAME",
    "db:seed:all": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:rollback": "node_modules/.bin/sequelize db:seed:undo:all",
    "db:seed:rollbake:one": "node_modules/.bin/sequelize db:seed:undo --seed $NAME",
    "db:seed:rollback:all": "node_modules/.bin/sequelize db:seed:undo:all",
    "make:migration": "node_modules/.bin/sequelize migration:create --name $NAME",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:one": "node_modules/.bin/sequelize db:migrate --name $NAME",
    "db:migrate:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "db:migrate:rollback:one": "node_modules/.bin/sequelize db:migrate:undo --name $NAME",
    "db:migrate:rollback:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:rollback:to": "node_modules/.bin/sequelize db:migrate:undo --to --name $NAME",
    "db:migrate:status": "node_modules/.bin/sequelize db:migrate:status",
    "make:model": "node_modules/.bin/sequelize model:generate --name $NAME"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/sequelize": "^10.0.0",
    "argon2": "^0.31.1",
    "bull": "^4.11.3",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "nestjs-command": "^3.1.4",
    "nodemailer": "^6.9.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.11",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestjs-seeder": "^0.3.2",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
